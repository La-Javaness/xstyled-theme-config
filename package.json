{
	"name": "@xstyled-theme/config",
	"version": "1.4.0",
	"description": "Tools to add xstyled-theme support to your app's tooling",
	"keywords": [
		"xstyled-theme",
		"xstyled",
		"theme",
		"styled-components",
		"designops"
	],
	"homepage": "https://github.com/La-Javaness/xstyled-theme-config#readme",
	"bugs": {
		"url": "https://github.com/La-Javaness/xstyled-theme-config/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:La-Javaness/xstyled-theme-config.git"
	},
	"license": "MIT",
	"author": "La Javaness <dev@lajavaness.com>",
	"main": "build/index.js",
	"files": [
		"build"
	],
	"scripts": {
		"build": "node scripts/build.js",
		"build:ci": "cross-env CI=true yarn build:prod",
		"build:next": "cross-env LJN_ENV=next yarn build",
		"build:prod": "cross-env LJN_ENV=production yarn build",
		"build:staging": "cross-env LJN_ENV=staging yarn build",
		"lint": "yarn eslint --ext .js,.jsx src src",
		"lint:fix": "yarn lint --fix",
		"prepublishOnly": "yarn test:ci && yarn build:ci",
		"prettier": "scripts/precommitHookPrettier.sh",
		"prettier:check": "npx prettier \"*/**/*.{js,jsx,ts,tsx,json,rc}\" --check",
		"prettier:fix": "npx prettier \"*/**/*.{js,jsx,ts,tsx,json,rc}\" --write",
		"test": "node scripts/test.js -u",
		"test:ci": "cross-env CI=true yarn test --watchAll=false --verbose --bail",
		"test:ci:coverage": "yarn test:ci --coverage",
		"test:coverage": "yarn test --coverage --watchAll=false"
	},
	"resolutions": {
		"@typescript-eslint/eslint-plugin": "2.14.0",
		"@typescript-eslint/parser": "2.14.0",
		"pretty-format": "24.9.0",
		"typescript": "3.7"
	},
